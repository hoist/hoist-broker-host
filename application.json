[{
  "name": "hoist-broker-host",
  "script": "./lib/application_event_host.js",
  "cwd": "./",
  "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
  "error_file": "/var/log/node/hoist-broker-host.stderr.log",
  "out_file": "/var/log/node/hoist-broker-host.stdout.log",
  "instances": "1",
  "merge_logs": true,
  "exec_mode": "fork_mode",
  "env": {
    "NODE_ENV": "production"
  }
}]
